{
  "name": "kymenus",
  "version": "0.1.0",
  "description": "A central repository of menus for Kentucky restuarants. Also a resource for picking somewere to eat.",
  "main": "bin/app.js",
  "scripts": {
    "removeUI": "rm -rf ./bin/ui/",
    "copyUI": "mkdir ./bin/ui/ && cp -r ./src/ui/* ./bin/ui/",
    "copyPrivate": "cp -r ./src/private/* ./bin/private/",
    "build": "rm -r ./bin && tsc && npm run copyUI && npm run copyPrivate",
    "cleanAllRun": "npm run build && node .",
    "cleanUIRun": "npm run removeUI && npm run copyUI && node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TrevorDBrown/KYMenus.git"
  },
  "author": "Trevor D. Brown",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TrevorDBrown/KYMenus/issues"
  },
  "homepage": "https://github.com/TrevorDBrown/KYMenus#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.1",
    "async": "^3.2.0",
    "bootstrap": "^4.5.3",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/express": "^4.17.8",
    "@types/jquery": "^3.5.5",
    "@types/mysql": "github:types/mysql",
    "ts-node-dev": "^1.0.0-pre.63",
    "typescript": "^4.0.3"
  }
}
